<div class="card column is-one-quarter">
  <div class="card-image">
    <figure class="image is-4by3">
      <img src=<%= c.thumbnail %>>
    </figure>
  </div>
  <div class="media-content">
    <a target="_blank" href=<%= c.link %>>
      <p class="card-title title is-4 is-size-6-touch">
      <%= c.title %>
      </p>
    </a>
  </div>
</div>
